<section class="map-section">
  <div class="map-wrapper">
    <iframe
      src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d200000!2d144.3!3d-38.15!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x6ad4114e3c53e7f1%3A0x5045675218ce6e0!2sGeelong%20VIC!5e0!3m2!1sen!2sau!4v1700000000000"
      width="100%"
      height="100%"
      style="border:0;"
      allowfullscreen=""
      loading="lazy"
      referrerpolicy="no-referrer-when-downgrade"
      title="EES Solar - Geelong, Victoria"
    ></iframe>
  </div>
  <div class="map-overlay"></div>
</section>

<style>
  .map-section {
    position: relative;
    width: 100%;
    height: 450px;
    overflow: hidden;
  }

  .map-wrapper {
    position: absolute;
    inset: 0;
    filter: grayscale(1) brightness(1.1) contrast(0.9);
  }

  .map-wrapper iframe {
    width: 100%;
    height: 100%;
    display: block;
  }

  .map-overlay {
    position: absolute;
    inset: 0;
    background: #0E2B47;
    mix-blend-mode: color;
    pointer-events: none;
  }

  @media (max-width: 700px) {
    .map-section {
      height: 350px;
    }
  }
</style>
