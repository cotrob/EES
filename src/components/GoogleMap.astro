<section class="map-section">
  <div class="map-wrapper">
    <iframe
      src="https://maps.google.com/maps?q=13+Nobility+Street+Moolap+VIC+3224+Australia&z=13&output=embed"
      width="100%"
      height="100%"
      style="border:0;"
      allowfullscreen=""
      loading="lazy"
      referrerpolicy="no-referrer-when-downgrade"
      title="EES Solar - Geelong, Victoria"
    ></iframe>
  </div>
  <div class="map-overlay"></div>
</section>

<style>
  .map-section {
    position: relative;
    width: 100%;
    height: 450px;
    overflow: hidden;
  }

  .map-wrapper {
    position: absolute;
    inset: 0;
    filter: grayscale(1) brightness(1.1) contrast(0.9);
  }

  .map-wrapper iframe {
    width: 100%;
    height: 100%;
    display: block;
  }

  .map-overlay {
    position: absolute;
    inset: 0;
    background: #0E2B47;
    opacity: 0.6;
    mix-blend-mode: color;
    pointer-events: none;
  }

  @media (max-width: 700px) {
    .map-section {
      height: 350px;
    }
  }
</style>
